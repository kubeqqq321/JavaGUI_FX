   <children>
      <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="400.0" layoutX="100.0" />
      <Button fx:id="playButton" layoutX="211.0" layoutY="313.0" mnemonicParsing="false" onAction="#playMedia" text="Play" />
      <Button fx:id="pauseButton" layoutX="274.0" layoutY="313.0" mnemonicParsing="false" onAction="#pauseMedia" text="Pause" />
      <Button fx:id="resetButton" layoutX="348.0" layoutY="313.0" mnemonicParsing="false" onAction="#resetMedia" text="Reset" />
   </children>
</AnchorPane>
